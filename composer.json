{
    "name": "firesphere/covidloi",
    "type": "silverstripe-recipe",
    "description": "COVID-19 LOI RSS Feed",
    "require": {
        "php": "^7.1 || ^8",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.8.0@stable",
        "silverstripe-themes/simple": "~3.2.0",
        "silverstripe/login-forms": "4.4.1@stable",
        "paquettg/php-html-parser": "^3",
        "silverstripe/mfa": "^4.3",
        "silverstripe/totp-authenticator": "^4.1",
        "silverstripe/webauthn-authenticator": "^4.2",
        "steadlane/silverstripe-cloudflare": "^2.0",
        "phptek/sentry": "^4.0",
        "matomo/matomo-php-tracker": "^3.0"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5",
        "silverleague/ideannotator": "^3",
        "lekoala/silverstripe-debugbar": "^2.0.6"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
