{
    "name": "firesphere/covidloi",
    "type": "silverstripe-recipe",
    "description": "COVID-19 LOI RSS Feed",
    "require": {
        "php": "^7.1 || ^8",
        "ext-json": "*",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "^4",
        "silverstripe-themes/simple": "^3",
        "silverstripe/login-forms": "^4",
        "paquettg/php-html-parser": "^3",
        "silverstripe/mfa": "^4",
        "silverstripe/totp-authenticator": "^4",
        "silverstripe/webauthn-authenticator": "^4",
        "steadlane/silverstripe-cloudflare": "^2",
        "phptek/sentry": "^4",
        "matomo/matomo-php-tracker": "^3",
        "silverstripe/googlesitemaps": "^2",
        "firesphere/solr-search": "^1"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5",
        "silverleague/ideannotator": "^3",
        "lekoala/silverstripe-debugbar": "^2"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
